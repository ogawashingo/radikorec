{
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/.next/**": true,
        "**/public/records/**": true,
        "**/.git/**": true,
        "**/.git": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/.next": true,
        "**/public/records": true,
        "**/.git": true
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/.next": true,
        "**/public/records": true,
        "**/*.db": true,
        "**/*.sqlite": true,
        "**/scanner.log": true,
